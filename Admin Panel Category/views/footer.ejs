<script>
        <% if (success && success.length > 0) { %>
        Swal.fire({
            title: "Success!",
            html: "<b><%= success %></b><br>Your password has been updated. Redirecting...",
            icon: "success",
            timer: 4500,
            timerProgressBar: true,
            showConfirmButton: false,
            position: 'top-end',
            toast: true,
            showClass: {
                popup: 'animate__animated animate__fadeInRight animate__faster'
            },
            hideClass: {
                popup: 'animate__animated animate__fadeOutRight animate__faster'
            },
            customClass: {
                popup: 'my-swal-success-popup',
                title: 'my-swal-success-title'
            }
        });
        <% } %>

        <% if (error && error.length > 0) { %>
        Swal.fire({
            title: "Oops!",
            html: "<b><%= error %></b><br>Please check your information and try again.",
            icon: "error",
            showConfirmButton: true,
            confirmButtonText: 'Try Again', /* More encouraging button text */
            confirmButtonColor: '#FF5722',
            showCloseButton: false,
            allowOutsideClick: false,
            allowEscapeKey: false,
            position: 'top-end',
            showClass: {
                popup: 'animate__animated animate__fadeInRight animate__faster'
            },
            hideClass: {
                popup: 'animate__animated animate__fadeOutRight animate__faster'
            },
            customClass: {
                popup: 'my-swal-error-popup',
                title: 'my-swal-error-title'
            }
        });
        <% } %>
</script>
