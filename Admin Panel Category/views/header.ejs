    <style>
        .my-swal-success-popup {
            border-left: 8px solid #4CAF50;
            border-radius: 12px;
            box-shadow: 0 8px 30px rgba(0, 0, 0, 0.2);
            font-family: 'Poppins', sans-serif;
            padding: 25px;
            background-image: linear-gradient(to right, #f0fdf4, #ffffff);
            /* Lighter success background */
            color: #2e7d32;
            /* Darker success text color */
        }

        .my-swal-success-title {
            color: #1b5e20;
            /* Even darker success title */
            font-weight: 700;
            font-size: 2em;
            /* Slightly larger title */
            margin-bottom: 12px;
        }

        .my-swal-error-popup {
            border-left: 8px solid #FF5722;
            border-radius: 12px;
            box-shadow: 0 8px 30px rgba(0, 0, 0, 0.2);
            font-family: 'Poppins', sans-serif;
            padding: 25px;
            background-image: linear-gradient(to right, #fdeded, #ffffff);
            /* Lighter error background */
            color: #d32f2f;
            /* Darker error text color */
        }

        .my-swal-error-title {
            color: #b71c1c;
            /* Even darker error title */
            font-weight: 700;
            font-size: 2em;
            margin-bottom: 12px;
        }

        .swal2-html-container {
            font-size: 1.2em;
            line-height: 1.7;
            color: #444;
            /* Slightly darker text */
        }

        .swal2-confirm.swal2-styled {
            background-color: #FF5722 !important;
            border-radius: 10px !important;
            /* More rounded button */
            font-weight: 600 !important;
            box-shadow: 0 4px 12px rgba(255, 87, 34, 0.4) !important;
            /* Stronger button shadow */
            transition: background-color 0.3s ease, transform 0.2s ease, box-shadow 0.2s ease !important;
            /* Smoother transitions */
        }

        .swal2-confirm.swal2-styled:hover {
            background-color: #f44336 !important;
            /* Slightly darker hover color */
            transform: translateY(-3px);
            /* More pronounced hover lift */
            box-shadow: 0 6px 15px rgba(255, 87, 34, 0.5) !important;
            /* Stronger hover shadow */
        }

        .form-control:focus {
            color: aqua;
        }

        @import url('https://fonts.googleapis.com/css2?family=Poppins:wght@400;600;700&display=swap');
    </style>

