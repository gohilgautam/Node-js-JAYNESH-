<%- include('../header') %>

    <div class="page-wrapper">
        <div class="page-breadcrumb">
            <div class="row">
                <div class="col-12 d-flex no-block align-items-center">
                    <h4 class="page-title">Tables</h4>
                    <div class="ms-auto text-end">
                        <nav aria-label="breadcrumb">
                            <ol class="breadcrumb">
                                <li class="breadcrumb-item"><a href="#">Home</a></li>
                                <li class="breadcrumb-item active" aria-current="page">
                                    Library
                                </li>
                            </ol>
                        </nav>
                    </div>
                </div>
            </div>
        </div>

        <div class="container-fluid">
            <div class="row">
                <div class="col-12">
                    <div class="card">
                        <div class="card-body">
                            <h5 class="card-title mb-0">ExtraCategory Data</h5>
                        </div>
                        <table class="table">
                            <thead>
                                <tr>
                                    <th scope="col">#</th>
                                    <th scope="col">Category Id</th>
                                    <th scope="col">SubCategory Id</th>
                                    <th scope="col">ExtraCategory Title</th>
                                    <th scope="col">Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <%allExtraCategory.map((data,index)=> { %>
                                    <tr>
                                        <th scope="row">
                                            <%= index+1 %>
                                        </th>
                                        <td>
                                            <%= data.category_id.c_title %>
                                        </td>
                                        <td>
                                            <%= data.subCategory_id.subcategory_title %>
                                        </td>

                                        <td>
                                            <%= data.extraCategory_title %>
                                        </td>
                                        <td>
                                            <a href="/subcategory/updateSubCategoryPage/" style="color: green;">Edit
                                            </a>

                                            <a href="/subcategory/deleteSubCategory/>" style="color: red;">Delete</a>
                                        </td>
                                    </tr>

                                    <% })%>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>


        <%- include('../footer') %>